{
  "name": "codeblitz-sample",
  "version": "1.0.0",
  "description": "codeblitz sample",
  "scripts": {
    "start": "npm run startup",
    "startup": "webpack serve --config webpack.config.js --env entry=startup",
    "startup:bundle": "webpack serve --config webpack.config.js --env entry=startup-bundle",
    "code": "webpack serve --config webpack.config.js --env entry=code",
    "main": "webpack serve --config webpack.config.js --env entry=main",
    "fs": "webpack serve --config webpack.config.js --env entry=filesystem",
    "editor": "webpack serve --config webpack.config.js --env entry=editor",
    "search": "webpack serve --config webpack.config.js --env entry=search",
    "zip": "webpack serve --config webpack.config.js --env entry=zip",
    "collaboration": "node ./collaboration/server & webpack serve --config webpack.config.js --env entry=collaboration",
    "conflicts": "webpack serve --config webpack.config.js --env entry=merge-editor"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "css-loader": "^6.7.3",
    "dotenv": "^8.2.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.1",
    "less": "^3.13.1",
    "less-loader": "^12.1.0",
    "node-polyfill-webpack-plugin": "^3.0.0",
    "path-browserify": "^1.0.1",
    "raw-loader": "^4.0.2",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.5.1",
    "typescript": "^5.5.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "@codeblitzjs/ide-core": "^2.2.1",
    "ahooks": "2",
    "antd": "^3.20.3",
    "file-saver": "^2.0.5",
    "jszip": "^3.10.1",
    "localforage": "^1.10.0",
    "lodash": "^4.17.21",
    "ot": "^0.0.15",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sha1": "^1.1.1",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2"
  },
  "resolutions": {
    "@rjsf/core": "5.5.2",
    "node-gyp": "npm:@favware/skip-dependency@latest",
    "nsfw": "npm:@favware/skip-dependency@latest",
    "spdlog": "npm:@favware/skip-dependency@latest",
    "node-pty": "npm:@favware/skip-dependency@latest",
    "@parcel/watcher": "npm:@favware/skip-dependency@latest",
    "keytar": "npm:@favware/skip-dependency@latest"
  },
  "repository": "git@github.com:opensumi/codeblitz-sample.git"
}
